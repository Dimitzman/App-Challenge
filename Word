  String [] tenLetters = new String[]{"playful", "bizarre", "sickened"};
  
  public String getNumberLetter(int d) { 
    if(d == 4){
      return fourletters[];
    }
    if(d == 5){
      return fiveLetters[];
    }
    else if(d == 6){
      return sixLetters[];
    }
    else if(d == 7){
      return sevenLetters[];
    }
    else if(d == 8){
      return eightLetters[];
    }
    else if(d == 9){
      return nineLetters[];
    }
    else if(d == 10){
      return tenLetters[];
    }
    else{
    return "Error";
    }
  
  }
  
  
  
